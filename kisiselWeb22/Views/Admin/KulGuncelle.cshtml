@model kisiselWeb22.Models.KullaniciBilgi


{
    ViewBag.Title = "KulGuncelle";
    Layout = "~/Views/_LayoutAdmin.cshtml";

}


<div id="content">
    <div id="content-header">
        <div id="breadcrumb">
            <a href="#" title="Go to Home" class="tip-bottom"><i class="icon-home"></i> Home</a>
            <a href="~/Admin1/Index" class="current">Tables</a>
        </div>
        <h1>Iletisim Tablosu</h1>
        <div>
            <h5>
                <a href="/Admin/Iletisim" style="width:200px; height:30px; margin-left:10px;border: 2px solid #4CAF50;border-radius: 16px; padding-top:10px;"
                   class="btn btn-info">Tabloya Git</a>
            </h5>
        </div>
    </div>
    <div class="container-fluid" style="position:static;">
        <hr>
        <div class="widget-box">
            <div class="widget-title">
                <span class="icon"> <i class="icon-th"></i> </span>
                <h5>Static table</h5>
            </div>

            <form action="/Admin/KulGuncelle" method="post" class="bg-light p-4 p-md-5 contact-form" style="margin-left:5px;">
                <div class="form-group">
                    @Html.LabelFor(m => m.Id)<br />
                    @Html.TextBoxFor(m => m.Id, new { @class = "form-control", style = "width:80%;" })

                    @Html.LabelFor(m => m.Ad)<br />
                    @Html.TextBoxFor(m => m.Ad, new { @class = "form-control", style = "width:80%;" })


                    @Html.LabelFor(m => m.Dtarih)
                    @Html.TextBoxFor(m => m.Dtarih, new { @class = "form-control", style = "width:80%;" })


                    @Html.LabelFor(m => m.Adress)
                    @Html.TextBoxFor(m => m.Adress, new { @class = "form-control", style = "width:80%;" })


                    @Html.LabelFor(m => m.ZipKod)
                    @Html.TextBoxFor(m => m.ZipKod, new { @class = "form-control", style = "width:80%;" })


                    @Html.LabelFor(m => m.Email)
                    @Html.TextBoxFor(m => m.Email, new { @class = "form-control", style = "width:80%;" })


                    @Html.LabelFor(m => m.Tel)
                    @Html.TextBoxFor(m => m.Tel, new { @class = "form-control", style = "width:80%;" })


                    @Html.LabelFor(m => m.Profil)
                    @Html.TextBoxFor(m => m.Profil, new { @class = "form-control", style = "width:80%;" })


                    @Html.LabelFor(m => m.Kaciklama)
                    @Html.TextBoxFor(m => m.Kaciklama, new { @class = "form-control", style = "width:80%;" })



                </div>
                <div> <input type="submit" class="btn btn-danger" value="Guncelle" /></div>
            </form>

        </div>
    </div>

</div>

