@model List<kisiselWeb22.Models.EgitimOZ>

<div class="resume-wrap d-flex ftco-animate">
    <div class="icon d-flex align-items-center justify-content-center">
        <span class="flaticon-ideas"></span>
    </div>
    <div class="text pl-3">
        @foreach (var item in Model)
        {
            if (item.Aralik != null)
            {
              <span class="date">@item.Aralik</span>
            }
            else
            {
                <span>Bos Alan</span>
            }
            if (item.Alan != null)
            {
             <h2>@item.Alan</h2>
            }
            else
            {
                <span>Bos Alan</span>
            }
            if (item.Okul != null)
            {
             <span class="position">@item.Okul</span>
            }
            else
            {
                <span>Bos Alan</span>
            }
            if(item.Aciklama != null)
            {

              <p>@item.Aciklama</p>

            }
            else
            {
                <span>Bos Alan</span>
            }
        }
    </div>
</div>
